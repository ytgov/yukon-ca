{
    "name": "evolvingweb/test",
    "description": "Yukon",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "acquia/blt": "^13.7",
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.7",
        "drupal/addtoany": "^2.0",
        "drupal/admin_toolbar": "^3.3",
        "drupal/advanced_help": "1.x-dev@dev",
        "drupal/ajax_comments": "1.x-dev@dev",
        "drupal/allowed_formats": "^1.3",
        "drupal/anchor_link": "^3.0@alpha",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/bigmenu": "^2.0@RC",
        "drupal/block_class": "^3.0",
        "drupal/ckeditor5_plugin_pack": "^1.1",
        "drupal/ckeditor5_show_block": "^1.14",
        "drupal/ckeditor_div_manager": "^3.0@alpha",
        "drupal/classy": "^1.0",
