{
    "name": "evolvingweb/test",
    "description": "Test",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "acquia/blt": "^13.7",
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.3",
        "drupal/allowed_formats": "^1.3",
        "drupal/classy": "^1.0",
        "drupal/config_split": "^1.4",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-project-message": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drupal/devel": "^5.1",
        "drupal/ds": "^3.15",
        "drupal/environment_indicator": "^4.0",
        "drupal/extlink": "^1.3",
        "drupal/fences": "^3.0",
        "drupal/field_group": "^3.1",
        "drupal/field_permissions": "^1.1",
        "drupal/focal_point": "^2.0@alpha",
        "drupal/link_attributes": "^1.11",
        "drupal/metatag": "^1.13",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/redis": "^1.5",
        "drupal/reroute_email": "^2.2",
        "drupal/seckit": "^2.0",
        "drupal/stage_file_proxy": "^1.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/ui_patterns": "^1.2",
        "drupal/viewsreference": "^1.7",
        "drupal/webform": "^6.0",
        "drupal/xmlsitemap": "^1.0",
        "evolvingweb/blinker_addons": "^2.4",
        "evolvingweb/blinker_core": "^2.0",
        "evolvingweb/drupal-ci": "^2.0",
        "evolvingweb/glider": "^3.6",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "extra": {
        "enable-patching": true,
        "patches": [],
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "gitignore": false,
            "locations": {
                "web-root": "./docroot"
            },
            "allowed-packages": [
                "pantheon-systems/drupal-integrations"
            ],
            "file-mapping": {
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                }
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "repositories": {
        "ew-packagist": {
            "type": "composer",
            "url": "https://packagist.ewdev.ca"
        },
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "acquia/blt": true,
            "acquia/blt-phpcs": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "drupal/core-project-message": true
        }
    },
    "autoload": {
        "psr-4": {
            "Evolvingweb\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    },
    "require-dev": {
        "acquia/blt-phpcs": "^1.0",
        "mglaman/phpstan-drupal": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0"
    }
}
