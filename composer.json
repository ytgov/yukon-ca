        "drupal/tmgmt": "^1.15",
        "drupal/tmgmt_diff": "^1.0@alpha",
        "drupal/token": "^1.11",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.2",
        "drupal/ui_patterns": "^1.2",
        "drupal/username_enumeration_prevention": "^1.3",
        "drupal/video_embed_field": "^2.5",
        "drupal/views_ajax_history": "^1.7",
        "drupal/views_bootstrap": "^5.4",
        "drupal/views_menu_children_filter": "^3.0@RC",
        "drupal/views_tree": "^2.0@RC",
        "drupal/views_year_filter": "^2.1",
        "drupal/viewsreference": "^1.7",
        "drupal/webform": "6.2.x-dev@dev",
        "drupal/workbench": "^1.4",
        "drupal/workbench_access": "^2.0",
        "drupal/workbench_email": "^3.0",
        "drupal/xmlsitemap": "^1.0",
        "evolvingweb/blinker_addons": "^2.4",
        "evolvingweb/blinker_core": "^2.0",
        "evolvingweb/drupal-ci": "^2.0",
        "evolvingweb/glider": "^3.6",
        "northernco/ckeditor5-anchor-drupal": "^0.5.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "pantheon-systems/drupal-integrations": "^10.0",
        "yukon/module_missing_message_fixer": "2.x-dev"
    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "2321071 - BaseFieldOverride fails to take into account ContentEntityInterface::bundleFieldDefinitions() when invoking onFieldDefinitionUpdate()": "https://www.drupal.org/files/issues/2023-11-02/2321071-71.patch",
                "3233527 - Include file fields in File source plugin if file_entity module is enabled": "https://www.drupal.org/files/issues/2022-04-04/drupal_core-3233527-Include-file-fields-in-File-source-plugin-if-file_entity-module-is-enabled.patch",
                "3154156 - Improve migration system performance: statically cache DrupalSqlBase::$systemData": "https://www.drupal.org/files/issues/2020-06-23/3154156-2.patch",
                "3264843 - Add support for negating node type(content type) condition for block visibility": "https://www.drupal.org/files/issues/2024-01-08/drupal-3264843-21.patch",
                "field config base": "https://www.drupal.org/files/issues/2024-04-25/remove_runtime_fieldsetting_11.patch"
            },
            "drupal/token": {
                "1614456 - [current-page:query:?] should fall back to empty string": "https://www.drupal.org/files/issues/2022-11-30/token-empty_current_page_query-1614456-28.patch"
            },
            "drupal/fontawesome": {
                "fontawesome icon name": "patches/fontawesome.patch"
            },
            "drupal/classy": {
                "3267438 - Add Classy ckeditor5-stylesheets config": "https://www.drupal.org/files/issues/2023-04-27/classy-ckeditor5-stylesheets-3267438-10.patch"
            },
            "drupal/field_group": {
                "field_group module": "https://www.drupal.org/files/issues/2024-08-21/field_group_fix-translations_label_description-3111107-50.patch"
            },
            "drupal/datetime_range": {
                "Date time format": "https://www.drupal.org/files/issues/2796151-06.patch"
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "gitignore": false,
            "locations": {
                "web-root": "./docroot"
            },
            "allowed-packages": ["pantheon-systems/drupal-integrations", "evolvingweb/drupal-ci"],
            "file-mapping": {
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "path": "docroot/core/assets/scaffold/files/default.settings.php",
                    "overwrite": false
                }
            }
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.ewdev.ca"
        },
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "yukon/module_missing_message_fixer",
                "type": "drupal-module",
                "version": "2.x-dev",
                "source": {
                    "url": "https://git.drupalcode.org/project/module_missing_message_fixer",
                    "type": "git",
                    "reference": "2.0.x"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "northernco/ckeditor5-anchor-drupal",
                "version": "0.5.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://registry.npmjs.org/@northernco/ckeditor5-anchor-drupal/-/ckeditor5-anchor-drupal-0.5.0.tgz",
                    "type": "tar"
                }
            }
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "acquia/blt": true,
            "acquia/blt-phpcs": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Evolvingweb\\": "blt/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "blt internal:create-project:init-repo"
        ],
        "nuke": [
            "rm -rf vendor composer.lock docroot/core docroot/modules/contrib docroot/profiles/contrib docroot/themes/contrib",
            "@composer clearcache --ansi",
            "@composer install --ansi"
        ]
    },
    "require-dev": {
        "acquia/blt-phpcs": "^1.0",
        "mglaman/phpstan-drupal": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0"
    }
}
